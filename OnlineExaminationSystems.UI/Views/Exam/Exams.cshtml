@model IEnumerable<OnlineExaminationSystems.UI.Models.Exam>
@{
    ViewBag.Title = "Exams";
}

<h2>Exams</h2>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model=>model.Id)</th>
            <th>@Html.DisplayNameFor(model => model.Name)</th>
            <th>@Html.DisplayNameFor(model => model.Duration)</th>
            <th>@Html.DisplayNameFor(model => model.QuestionCount)</th>
            <th>@Html.DisplayNameFor(model => model.StartedDate)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var exam in Model)
        {
            <tr>
                <td>@exam.Id</td>
                <td>@exam.Name</td>
                <td>@exam.Duration</td>
                <td>@exam.QuestionCount</td>
                <td>@exam.StartedDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>


